{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/favicon\\.png",
      "dest": "/public/favicon.png"
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/(css|js|icons|scripts|sounds|uploads)/.*",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*)\\.(png|jpg|jpeg|gif|svg|ico|css|js|woff|woff2|ttf|eot|mp3|webp)$",
      "dest": "/public/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}
